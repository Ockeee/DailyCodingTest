public class Day1_FoodFight {
    public String solution(int[] food) {
        String answer = "0";

        for(int i = 1; i<food.length; i++) {
            int halfLength = food[i] / 2;
            for (int j = 0; j < halfLength; j++) {
                answer = i + answer + i;
            }
        }

        return answer;
    }
}
